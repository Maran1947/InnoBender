<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.innobender">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InnoBender">
        <activity
            android:name=".TimelineTasksActivity"
            android:label="@string/title_activity_timeline_tasks"
            android:theme="@style/Theme.InnoBender.NoActionBar"></activity>
        <activity android:name=".CurrentTimelineActivity" />
        <activity android:name=".PendingRequestActivty" />
        <activity android:name=".CreateTimeline" />
        <activity android:name=".JoinTimeline" />
        <activity android:name=".LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>